{
  "name": "dir-snap",
  "version": "0.3.0",
  "description": "This compares the contents of a directory against a previous snapshot.",
  "keywords": [
    "directory",
    "compare",
    "folders",
    "snapshot",
    "dir"
  ],
  "main": "./core/snapshot.js",
  "bin": {
    "dirsnap": "./bin/dirsnap"
  },
  "author": "Lance Turri",
  "repository": {
    "type": "git",
    "url": "https://github.com/LanceTurri/dir-snap"
  },
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "debug": "DEBUG=* node --inspect-brk ./bin/dirsnap-reference --folder test/testParent"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "cliff": "^0.1.10",
    "commander": "^3.0.1",
    "debug": "^4.0.0",
    "json-diff": "^0.5.2",
    "md5": "^2.2.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.0.0"
  },
  "preferGlobal": true,
  "license": "MIT"
}
